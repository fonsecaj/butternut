{
  "name": "@sekoya/storybook",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/storybook/src",
  "prefix": "sb",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "build": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/apps/storybook",
        "configDir": "{projectRoot}/src",
        "styles": ["{projectRoot}/src/preview.css"],
        "browserTarget": "@sekoya/storybook:build",
        "compodoc": false
      }
    },
    "start": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 6007,
        "configDir": "{projectRoot}/src",
        "browserTarget": "@sekoya/storybook:build",
        "compodoc": false
      }
    }
  },
  "implicitDependencies": ["@sekoya/ui"]
}
